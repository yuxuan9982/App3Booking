<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:orientation="vertical">
    <TableLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:column_count="4"
        ohos:row_count="4"
        ohos:orientation="horizontal"
        ohos:alignment_type="align_edges"
        >
        <DependentLayout
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:left_margin="$string:margin"
            ohos:top_margin="$string:margin"
            >
            <Image
                ohos:height="$string:normal_len"
                ohos:width="$string:normal_len"
                ohos:id="$+id:i1"
                ohos:background_element="$media:icon"
                />
            <Text
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:text="工资"
                ohos:text_size="$string:small_text"
                ohos:below="$id:i1"
                ohos:text_alignment="center"
                />
        </DependentLayout>
        <DependentLayout
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:left_margin="$string:margin"
            ohos:top_margin="$string:margin"
            >
            <Image
                ohos:height="$string:normal_len"
                ohos:width="$string:normal_len"
                ohos:id="$+id:i2"
                ohos:background_element="$media:icon"
                />
            <Text
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:text="兼职"
                ohos:text_size="$string:small_text"
                ohos:below="$id:i2"
                ohos:text_alignment="center"
                />
        </DependentLayout>
        <DependentLayout
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:left_margin="$string:margin"
            ohos:top_margin="$string:margin"
            >
            <Image
                ohos:height="$string:normal_len"
                ohos:width="$string:normal_len"
                ohos:id="$+id:i3"
                ohos:background_element="$media:icon"
                />
            <Text
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:text="理财"
                ohos:text_size="$string:small_text"
                ohos:below="$id:i3"
                ohos:text_alignment="center"
                />
        </DependentLayout>

        <DependentLayout
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:left_margin="$string:margin"
            ohos:top_margin="$string:margin"
            >
            <Image
                ohos:height="$string:normal_len"
                ohos:width="$string:normal_len"
                ohos:id="$+id:i4"
                ohos:background_element="$media:icon"
                />
            <Text
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:text="礼金"
                ohos:text_size="$string:small_text"
                ohos:below="$id:i4"
                ohos:text_alignment="center"
                />
        </DependentLayout>

        <DependentLayout
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:left_margin="$string:margin"
            ohos:top_margin="$string:margin"
            >
            <Image
                ohos:height="$string:normal_len"
                ohos:width="$string:normal_len"
                ohos:id="$+id:i5"
                ohos:background_element="$media:icon"
                />
            <Text
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:text="其他"
                ohos:text_size="$string:small_text"
                ohos:below="$id:i5"
                ohos:text_alignment="center"
                />
        </DependentLayout>
    </TableLayout>
</DirectionalLayout>